<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL" 
                  xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI"
                  xmlns:dc="http://www.omg.org/spec/DD/20100524/DC"
                  xmlns:di="http://www.omg.org/spec/DD/20100524/DI"
                  xmlns:camunda="http://camunda.org/schema/1.0/bpmn"
                  id="TestProcessDefinitions" 
                  targetNamespace="http://example.com/test"
                  exporter="Camunda Modeler" 
                  exporterVersion="5.0.0">
  
  <bpmn:process id="test-simple-process" name="Test Simple Process" isExecutable="true">
    <bpmn:startEvent id="start-event" name="Start">
      <bpmn:outgoing>flow-start-to-task</bpmn:outgoing>
    </bpmn:startEvent>
    
    <bpmn:userTask id="user-task" name="Test User Task" camunda:assignee="demo">
      <bpmn:incoming>flow-start-to-task</bpmn:incoming>
      <bpmn:outgoing>flow-task-to-end</bpmn:outgoing>
    </bpmn:userTask>
    
    <bpmn:endEvent id="end-event" name="End">
      <bpmn:incoming>flow-task-to-end</bpmn:incoming>
    </bpmn:endEvent>
    
    <bpmn:sequenceFlow id="flow-start-to-task" sourceRef="start-event" targetRef="user-task"/>
    <bpmn:sequenceFlow id="flow-task-to-end" sourceRef="user-task" targetRef="end-event"/>
  </bpmn:process>

  <bpmn:process id="test-automated-process" name="Test Automated Process" isExecutable="true">
    <bpmn:startEvent id="auto-start" name="Auto Start">
      <bpmn:outgoing>flow-auto-start-to-end</bpmn:outgoing>
    </bpmn:startEvent>
    
    <bpmn:endEvent id="auto-end" name="Auto End">
      <bpmn:incoming>flow-auto-start-to-end</bpmn:incoming>
    </bpmn:endEvent>
    
    <bpmn:sequenceFlow id="flow-auto-start-to-end" sourceRef="auto-start" targetRef="auto-end"/>
  </bpmn:process>

  <!-- Диаграмма для визуализации -->
  <bpmndi:BPMNDiagram id="BPMNDiagram_1">
    <bpmndi:BPMNPlane id="BPMNPlane_1" bpmnElement="test-simple-process">
      <bpmndi:BPMNShape id="BPMNShape_StartEvent" bpmnElement="start-event">
        <dc:Bounds x="179" y="99" width="36" height="36"/>
        <bpmndi:BPMNLabel>
          <dc:Bounds x="185" y="142" width="24" height="14"/>
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="BPMNShape_UserTask" bpmnElement="user-task">
        <dc:Bounds x="270" y="77" width="100" height="80"/>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="BPMNShape_EndEvent" bpmnElement="end-event">
        <dc:Bounds x="432" y="99" width="36" height="36"/>
        <bpmndi:BPMNLabel>
          <dc:Bounds x="440" y="142" width="20" height="14"/>
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNEdge id="BPMNEdge_SequenceFlow_1" bpmnElement="flow-start-to-task">
        <di:waypoint x="215" y="117"/>
        <di:waypoint x="270" y="117"/>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="BPMNEdge_SequenceFlow_2" bpmnElement="flow-task-to-end">
        <di:waypoint x="370" y="117"/>
        <di:waypoint x="432" y="117"/>
      </bpmndi:BPMNEdge>
    </bpmndi:BPMNPlane>
  </bpmndi:BPMNDiagram>
</bpmn:definitions>
